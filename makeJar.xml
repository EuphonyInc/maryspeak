<?xml version="1.0" encoding="UTF-8"?>
<project name="maryspeak.makejar" default="makejar" basedir=".">
  <target name ="makejar" description="Create a jar for maryspeak">
    <jar destfile="maryspeak.jar" >
    	<fileset dir="bin/">
    		<include name="**/*.class"/>
		</fileset>
      	<fileset dir="src/">
       		<include name="maryspeak.txt"/>
   		</fileset>
    </jar>
  </target>
</project>
